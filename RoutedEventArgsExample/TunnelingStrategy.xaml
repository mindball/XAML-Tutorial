<Window x:Class="RoutedEventArgsExample.TunnelingStrategy"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RoutedEventArgsExample"
        mc:Ignorable="d"
        Title="TunnelingStrategy" Height="450" Width="800">
    <StackPanel>
        <Label Name="DisplayLabel" FontSize="20" Height="40"></Label>
        <Button x:Name="OuterButton"
                PreviewMouseDown="OuterButton_PreviewMouseDown"
                Height="150"
                Width="200"
                >
            <Button Name="InnerButton"
                    Height="100"
                    Width="100"
                    Click="InnerButton_Click">
                Inner Button
            </Button>
        </Button>
    </StackPanel>
</Window>
